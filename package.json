{
  "name": "cypresslearning",
  "version": "1.0.0",
  "description": "This is repo created for Cypress Automation tool learning",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "assignment1": "npx cypress run --spec ./cypress/e2e/Day5/assignmentOne.cy.js --browser chrome --headed",
    "assignment2": "npx cypress run --spec ./cypress/e2e/Day5/assignmentTwo.cy.js --browser chrome --headed",
    "assignment3": "npx cypress run --spec ./cypress/e2e/Day5/assignmentThree.cy.js --browser chrome --headed",
    "assignment4": "npx cypress run --spec ./cypress/e2e/Day5/assignmentFour.cy.js --browser chrome --headed",
    "assignment5": "npx cypress run --spec ./cypress/e2e/Day5/assignmentFive.cy.js --browser chrome --headed",
    "mochawesomeReport": "npx mochawesome-merge ./cypress/results/.jsons/*.json -o  mochawesomeReport.json",
    "generateReport": "npx marge mochawesomeReport.json",
    "assignment5_with_allure": "npx cypress run --spec ./cypress/e2e/Day5/assignmentFive.cy.js --browser chrome --headed --env allure=true",
    "allureReport": "allure serve ./allure-results"
  },
